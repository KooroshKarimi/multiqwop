{
  "name": "qwop-multiplayer",
  "version": "1.0.0",
  "description": "QWOP Multiplayer Web Game - A modern web implementation of the classic QWOP game with global highscore system",
  "main": "index.html",
  "scripts": {
    "start": "live-server --port=3000 --open=/",
    "build": "webpack --mode=production",
    "dev": "webpack --mode=development --watch",
    "test": "jest",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write src/**/*.{js,css,html}"
  },
  "keywords": [
    "qwop",
    "game",
    "multiplayer",
    "physics",
    "web",
    "canvas",
    "planck.js"
  ],
  "author": "QWOP Multiplayer Team",
  "license": "MIT",
  "devDependencies": {
    "live-server": "^1.2.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "babel-loader": "^9.1.3",
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "style-loader": "^3.3.3",
    "css-loader": "^6.8.1",
    "eslint": "^8.45.0",
    "prettier": "^3.0.0",
    "jest": "^29.6.2"
  },
  "dependencies": {
    "planck": "^0.4.0",
    "firebase": "^10.1.0",
    "express": "^4.18.2"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}